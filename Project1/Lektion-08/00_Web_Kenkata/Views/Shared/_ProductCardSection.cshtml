@model ProductCardSectionModel

<section class="productcard-section">
    <div class="container">
        <partial name="_SectionTitle" model="@Model.SectionTitle" />

        <div class="row row-cols-1 row-cols-md-4 g-4">

            @foreach (var product in Model.Products)
            {
                <partial name="_ProductCard" model="product" />
            }

        </div>
    </div>
</section>